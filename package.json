{
  "name": "iop-snark-library",
  "version": "1.0.1",
  "description": "A native Node.js wrapper for the libiop zk-SNARK library.",
  "main": "index.js",
  "private": false,
  "gypfile": false,
  "scripts": {
    "install": "node-pre-gyp install || npm run build",
    "build": "cmake-js build",
    "package": "node-pre-gyp package"
  },
  "dependencies": {
    "node-addon-api": "^8.0.0",
    "node-pre-gyp": "^0.17.0"
  },
  "devDependencies": {
    "cmake-js": "^7.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amogusdrip285/iop-snark-library.git"
  },
  "license": "MIT",
  "binary": {
    "module_name": "addon",
    "module_path": "./build/Release",
    "host": "https://github.com/amogusdrip285/iop-snark-library/releases/download/",
    "remote_path": "{version}",
    "package_name": "{module_name}-v{version}-{platform}-{arch}.tar.gz",
    "napi_build_version": 9
  }
}